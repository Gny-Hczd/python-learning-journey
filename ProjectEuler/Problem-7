a = 2
count = 0
n=100
while count<=n:
    for i in range(2, a):
        if a % i == 0:
            break
    else:
        count+=1
    if count==n:
                print(f"{count} -ci sade eded: {a}")
                break
    a+=1

#This code finds the 100th prime number.  
